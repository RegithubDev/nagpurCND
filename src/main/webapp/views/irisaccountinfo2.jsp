        <!-- form -->
        <form id="jquery-val-form" class="validate-form mt-2 pt-50" action="<%=request.getContextPath() %>/update-user-self-iris"  method="post" novalidate="novalidate" >
          <div class="row">
            <div class="col-12 col-sm-6 mb-1">
              <label class="form-label" for="accountFirstName">Name</label>
              <input
                type="text"
                class="form-control"
                id="user_name"
                name="user_name"
                placeholder="John"
                value="${UserDetails.user_name }"
                data-msg="Please enter first name"
              /> 
            </div>
            <div class="mb-1 col-md-6">
             <input type="hidden" id="id" name="id" value="${UserDetails.id }" />
             <label class="form-label" for="basic-default-name">Name</label>  <span class=re-text>*</span>
              <input type="text" class="form-control" id="user_name" name="user_name" placeholder="Name" value="${UserDetails.user_name }">
            </div>
            <div class="col-12 col-sm-6 mb-1">
              <label class="form-label" for="accountEmail">Email</label>
              <input disabled
                type="email"
                class="form-control"
                id="accountEmail"
                placeholder="Email"
                value="${UserDetails.email_id }"
              />
            </div>
            
            <div class="col-12 col-sm-6 mb-1">
              <label class="form-label" for="accountPhoneNumber">Phone Number</label>
              <input
                type="text"
                class="form-control account-number-mask"
                id="accountPhoneNumber"
                name="mobile_number"
                placeholder="Phone Number"
                value="${UserDetails.mobile_number }"
              />
            </div>
          
           <div class="col-md-12 col-12 text-center mt-2">
